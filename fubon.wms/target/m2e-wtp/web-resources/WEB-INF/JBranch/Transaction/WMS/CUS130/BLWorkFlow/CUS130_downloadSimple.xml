<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE workflow PUBLIC "-//OpenSymphony Group//DTD OSWorkflow 2.8//EN" 
"http://www.opensymphony.com/osworkflow/workflow_2_8.dtd">
<workflow>
	<initial-actions>
		<action id="100" name="開始">
			<results>
				<unconditional-result old-status="Finished"
					status="Queued" step="1" />
			</results>
		</action>
	</initial-actions>

	<steps>
		<step id="1" name="步驟1">
			<actions>
				<action id="1" name="動作1">
					<pre-functions>
						<function type="spring">
							<arg name="bean.name">cus130</arg>
							<arg name="method">downloadSimple</arg>
						</function>
					</pre-functions>
					<results>
						<unconditional-result old-status="Finished" status="Queued" step="2" owner="User100" />
					</results>
				</action>
			</actions>
		</step>

		<step id="2" name="停止" />
	</steps>
</workflow>