<?xml version="1.0" encoding="UTF-8"?><beans xmlns="http://www.springframework.org/schema/beans"       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"       xmlns:util="http://www.springframework.org/schema/util"       xmlns:p="http://www.springframework.org/schema/p"       xsi:schemaLocation="http://www.springframework.org/schema/beans  http://www.springframework.org/schema/beans/spring-beans-2.0.xsd  http://www.springframework.org/schema/util  http://www.springframework.org/schema/util/spring-util-2.0.xsd">      <bean id="system" class="com.systex.jbranch.platform.common.dataManager.System">        <property name="path" ref="Path"/>        <property name="configFileName" ref="ConfigFileName"/>        <property name="defaultValue" ref="DefaultValue"/>        <property name="info" ref="Info"/>    </bean>        <util:map id="Path" map-class="java.util.HashMap">        <entry key="log" value="log/"/>        <entry key="fileTransferIN" value="fileTransfer/IN/"/>        <entry key="fileTransferOUT" value="fileTransfer/OUT/"/>        <entry key="config" value="Config/"/>        <entry key="temp" value="temp/"/>        <entry key="language" value="Language"/>        <entry key="OutputReport" value="Transaction/"/>        <entry key="HTG" value="Platform/HTG/"/>    </util:map>        <util:map id="ConfigFileName" map-class="java.util.HashMap">    </util:map>        <util:map id="DefaultValue" map-class="java.util.HashMap">        <entry key="timeout" value="90"/>        <entry key="sectionNum" value="2"/>        <entry key="language" value="zh_TW"/>        <!-- 上傳檔案size限制 	-1:無限制 -->        <entry key="uploadFileMaxSize" value="-1"/>        <!-- 超過此size則存放於hd  -->        <entry key="uploadFileSizeThresHold" value="4096"/>    </util:map>    <bean id="apServerInfo" class="com.systex.jbranch.host.APServerInfo"/>    <util:map id="Info" map-class="java.util.HashMap">        <entry key="apServerName" value="#{apServerInfo.getServerName()}"/>        <entry key="description" value="#{T (java.net.InetAddress).getLocalHost().getHostName()}"/>        <entry key="type" value="S"/>        <!-- c:工作站;S:伺服器;D:開發工作站 -->        <entry key="mode" value="#{serverModeFactory.getValue()}"/>        <!-- R:正式環境 ;D:開發環境 -->        <entry key="os" value="windows"/>        <entry key="pwdKey" value="ABC123CBDFEF887D"/>        <entry key="url.loginpage" value="?authuid=%s" /> 		<entry key="url.system.fp" value="http://172.16.241.167/fb/OutsideLogin.do" />   		<entry key="url.help.document" value="#{urlHelpDocumentFactory.getValue()}" />  		<entry key="seal.psAddress" value="#{sealPsAddressFactory.getValue()}"/> 		<entry key="seal.udpPort" value="#{sealUdpPortFactory.getValue()}"/> 		<entry key="seal.psPort" value="#{sealPsPortFactory.getValue()}"/> 		<entry key="seal.ssAddress" value="#{sealSsAddressFactory.getValue()}"/> 		<entry key="seal.ssPort" value="#{sealSsPortFactory.getValue()}"/> 		<entry key="url.system.adws" value="#{urlSystemAdwsFactory.getValue()}"/>        		<entry key="url.bms.mainpage" value="#{urlBmsMainpageFactory.getValue()}"/> 		<entry key="url.ssoCheck" value="#{urlSsoCheckFactory.getValue()}"/>             				           </util:map>    <bean id="dataManager" class="com.systex.jbranch.platform.common.dataManager.impl.DataManagerImpl"          p:system-ref="system"          p:realPath="#{serverInfo.serverPath}"          p:root="#{new java.io.File(serverInfo.serverPath, serverInfo.jbranchRoot).getAbsolutePath()}"/><!--     <bean id="dataManager" class="com.systex.jbranch.platform.common.dataManager.impl.db.DataManagerDB" --><!--          p:system-ref="system" --><!--          p:realPath="#{serverInfo.serverPath}" --><!--          p:root="#{new java.io.File(serverInfo.serverPath, serverInfo.jbranchRoot).getAbsolutePath()}" --><!--          p:template-ref="hibernateTemplate"> --><!--     </bean> -->    <bean id="hibernateTemplate" class="org.springframework.orm.hibernate4.HibernateTemplate"          p:sessionFactory-ref="defaultSessionFactory" />    <bean id="userPlatformVarManager" class="com.systex.jbranch.platform.common.dataManager.impl.db.UserPlatformVarManager" p:template-ref="hibernateTemplate"/>    <bean id="userBizlogicVarManager" class="com.systex.jbranch.platform.common.dataManager.impl.db.UserBizlogicVarManager" p:template-ref="hibernateTemplate"/>    <bean id="branchPlatformVarManager" class="com.systex.jbranch.platform.common.dataManager.impl.db.BranchPlatformVarManager" p:template-ref="hibernateTemplate"/>    <bean id="branchBizlogicVarManager" class="com.systex.jbranch.platform.common.dataManager.impl.db.BranchBizlogicVarManager" p:template-ref="hibernateTemplate"/>    <bean id="sysPlatformVarManager" class="com.systex.jbranch.platform.common.dataManager.impl.db.SysPlatformVarManager" p:template-ref="hibernateTemplate"/>    <bean id="sysBizlogicVarManager" class="com.systex.jbranch.platform.common.dataManager.impl.db.SysBizlogicVarManager" p:template-ref="hibernateTemplate"/>    <bean id="systemVarManager" class="com.systex.jbranch.platform.common.dataManager.impl.db.SystemVarManager" p:template-ref="hibernateTemplate"/>        <bean id="wmsUser" class="com.systex.jbranch.platform.server.info.fubonuser.WmsUser"/>    <bean id="def_branch_factory" class="com.systex.jbranch.platform.server.info.fubonbranch.WmsBranchFactory"/>    <!-- <bean id="def_branch_factory" class="com.systex.jbranch.platform.server.info.branch.BranchFactory"/> --></beans>