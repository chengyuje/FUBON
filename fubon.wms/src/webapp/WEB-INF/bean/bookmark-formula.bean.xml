<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


    <bean id="bk_limit_length" class="com.systex.jbranch.office.formula.LimitLength"
          p:docCharSet="MS950" scope="prototype"/>

    <bean id="bk_substring" class="com.systex.jbranch.office.formula.SubString" scope="prototype"
          p:docCharSet="MS950"/>

	<bean id="bk_funcation" class="com.systex.jbranch.office.formula.Funcation" scope="prototype"/>
          
	<bean id="bk_number_format" class="com.systex.jbranch.office.formula.NumberFormat" scope="prototype"/>
          
    <bean id="bk_date_format" class="com.systex.jbranch.office.formula.DateFormat" scope="prototype"/>
          
	<bean id="bk_padding" class="com.systex.jbranch.office.formula.Padding" scope="prototype"/>

	<bean id="bk_numberToWord" class="com.systex.jbranch.office.formula.NumberToWord" scope="prototype"/>
</beans>