package com.systex.jbranch.platform.common.dataManager;

import java.io.Serializable;

public class HostVO implements Serializable {
	private boolean EOF;
	private String inputBuffer;
	private String outputBuffer;
	/**
	 * @return the eOF
	 */
	public boolean isEOF() {
		return EOF;
	}
	/**
	 * @return the inputBuffer
	 */
	public String getInputBuffer() {
		return inputBuffer;
	}
	/**
	 * @return the outputBuffer
	 */
	public String getOutputBuffer() {
		return outputBuffer;
	}
	/**
	 * @param eof the eOF to set
	 */
	public void setEOF(boolean eof) {
		EOF = eof;
	}
	/**
	 * @param inputBuffer the inputBuffer to set
	 */
	public void setInputBuffer(String inputBuffer) {
		this.inputBuffer = inputBuffer;
	}
	/**
	 * @param outputBuffer the outputBuffer to set
	 */
	public void setOutputBuffer(String outputBuffer) {
		this.outputBuffer = outputBuffer;
	}
}
