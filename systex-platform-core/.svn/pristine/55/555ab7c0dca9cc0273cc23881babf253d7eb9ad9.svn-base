package com.systex.jbranch.platform.server;

public class HSM {
	private String ip;
	private String id;
	private String pwd;
	private String ppKey;
	private String cKey;
	public String getIp()
	{
		return ip;
	}
	public void setIp(String address)
	{
		this.ip=address;
	}
	public String getId()
	{
		return id;
	}
	public void setId(String acc)
	{
		this.id=acc;
	}
	public String getPwd()
	{
		return pwd;
	}
	public void setPwd(String pass)
	{
		this.pwd=pass;
	}
	public String getPpKey()
	{
		return ppKey;
	}
	public void setPpKey(String pp)
	{
		this.ppKey=pp;
	}
	public String getCKey()
	{
		return cKey;
	}
	public void setCJKey(String c)
	{
		this.cKey=c;
	}

}
